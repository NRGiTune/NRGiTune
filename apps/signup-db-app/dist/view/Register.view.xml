<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="supabase.app.controller.Register">
  <Page title="Registo">
    <content>
      <VBox class="sapUiSmallMargin">
        <Input id="regName" placeholder="Nome"/>
        <Input id="regEmail" placeholder="Email" type="Email"/>
        <Input id="regPassword" placeholder="Password" type="Password"/>
        <Button text="Registar" press=".onRegister"/>
      </VBox>
    </content>
  </Page>
</mvc:View>
