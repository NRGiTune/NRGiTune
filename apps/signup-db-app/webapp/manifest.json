{
  "_version": "1.12.0",
  "sap.app": {
    "id": "supabase.app",
    "type": "application",
    "title": "OpenUI5 + Supabase"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "supabase.app.view.App",
      "type": "XML",
      "id": "appView",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.84.0",
      "libs": {
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "supabase.app.view",
        "controlId": "app",
        "transition": "slide",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "Login",
          "target": "Login"
        },
        {
          "pattern": "register",
          "name": "Register",
          "target": "Register"
        },
        {
          "pattern": "profile",
          "name": "Profile",
          "target": "Profile"
        }
      ],
      "targets": {
        "Login": {
          "viewName": "Login"
        },
        "Register": {
          "viewName": "Register"
        },
        "Profile": {
          "viewName": "Profile"
        }
      }
    }
  }
}