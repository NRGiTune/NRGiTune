<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="supabase.app.controller.Login">
  <Page title="Login">
    <content>
      <VBox class="sapUiSmallMargin">
        <Input id="email" placeholder="Email" type="Email"/>
        <Input id="password" placeholder="Password" type="Password"/>
        <Button text="Entrar" press=".onLogin"/>
        <Button text="Registar" press=".onGoRegister"/>
      </VBox>
    </content>
  </Page>
</mvc:View>
